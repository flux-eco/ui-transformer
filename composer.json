{
  "name": "flux-eco/ui-transformer",
  "description": "Component for flux-capacitor apps developed by fluxlabs ag",
  "version": "1.1.0",
  "type": "flux-app",
  "keywords": [
    "flux-eco",
    "ui-transformer",
    "fluxlabs"
  ],
  "homepage": "https://fluxlabs.ch",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "fluxlabs ag",
      "email": "support@fluxlabs.ch",
      "homepage": "https://fluxlabs.ch",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "support@fluxlabs.ch"
  },
  "require": {
    "php": ">=8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-yaml": "*",
    "flux-eco/dot-env": ">=1.2.0"
  },
  "autoload": {
    "files": [
      "fn/getPageDefinition.php",
      "fn/getPages.php"
    ],
    "psr-4": {
      "FluxEco\\UiTransformer\\": [
        "src/"
      ],
      "fluxUiTransformer\\": [
        "fn/"
      ]
    }
  },
  "config": {
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "sort-packages": true,
    "platform-check": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  }
}
